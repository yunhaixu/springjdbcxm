/**/

.kc-item img {
    position: absolute;
    pointer-events: none;
    width: 100%;
    box-shadow: rgba(0, 0, 0, 0.4) 0 1px 4px;
    border: 1px solid #bebebe;
}

.kc-wrap > div:nth-child(2) {
    display: none !important;
}

.index-tab {
    width: 862px;
    padding-top: 40px;
    margin: 0 auto;
}

.index-tab .four-link {
    margin: 0 11px;
    height: 82px;
}

.index-tab .four-link ul {
    margin-left: -13px;
}

.index-tab .four-link li {
    float: left;
    margin-right: 13px;
}

.index-tab .four-link a {
    display: block;
    width: 93px;
    height: 82px;
    line-height: 82px;
    font-size: 17px;
    padding-left: 107px;
    background: url(../images/index/four-link1.png) left center no-repeat;
}

.index-tab .four-link a:hover {
    background-image: url(../images/index/four-link1-h.png);
}

.index-tab .four-link a.two {
    background-image: url(../images/index/four-link2.png);
}

.index-tab .four-link a.two:hover {
    background-image: url(../images/index/four-link2-h.png);
}

.index-tab .four-link a.three {
    background-image: url(../images/index/four-link3.png);
}

.index-tab .four-link a.three:hover {
    background-image: url(../images/index/four-link3-h.png);
}

.index-tab .four-link a.four {
    background-image: url(../images/index/four-link4.png);
}

.index-tab .four-link a.four:hover {
    background-image: url(../images/index/four-link4-h.png);
}


/*tab */

.tabview {
    margin-top: 45px;
}

.tabview-hd ul {
    margin-right: -4px;
}

.tab-items {
    float: left;
    width: 284px;
    height: 58px;
    text-align: center;
    line-height: 58px;
    margin-right: 4px;
    font-size: 17px;
    cursor: pointer;
    background-color: #f2f2f2;
}

.tab-items>i {
    position: absolute;
    margin-top: -7px;
    margin-left: 125px;
    width: 28px;
    display: none;
    height: 20px;
    background-image: url(../images/index/index-tab-sanjiao.png);
}

.tab-items.active>i {
    display: block;
}

.tab-items.active {
    background-color: #70c49a;
    color: #fff;
    font-weight: bold;
}


