.content-left {
    float: left;
    width: 368px;
    margin-top: 20px;
    margin-left: 20px;
}

.content-right {
    float: right;
    width: 622px;
    margin-top: 20px;
    margin-right: 20px;
}

/* 轮播图 */
.wb-slider {
    position: relative;
    font-family: arial;
    font-size: 14px;
    overflow: hidden;
}
.wb-slider-conbox {
    overflow: hidden;
}
.wb-slider-ctag {
    display: block;
    position: relative;
}
.wb-slider-ctag img {
    display: block;
    width: 100%;
    height: 100%;
}
.wb-slider-mask {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 38px;
    line-height: 38px;
    font-size: 12px;
    font-family: "Microsoft Yahei";
    color: #fff;
    background: url(../images/freeTrade-zone/mask-bg.png);
}
.wb-slider-mask .ellipsis {
    display: block;
    margin-left: 10px;
    width: 213px;
    color: #fff;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.wb-slider-switcher {
    position: absolute;
    right: 0;
    bottom: 9px;
    height: 18px;
}
.wb-slider-stag {
    float: left;
    margin-right: 3px;
    height: 18px;
    width: 25px;
    font-size: 12px;
    line-height: 18px;
    text-align: center;
    color: #fff;
    background-color: #898989;
    cursor: pointer;
}

.wb-slider-stag.cur {
    background-color: #ff8202;
}


/*content-right*/
.location-name {
    height: 40px;
    padding-left: 60px;
    margin-bottom: 15px;
    background: url(../images/freeTrade-zone/location-nameBg.png) 4px center no-repeat;
}
.location-name>p {
    padding-top: 4px;
}

/*tab*/
.tab-panel {
    height: 154px;
    padding-top: 6px;
    line-height: 26px;
}
.tab-panel .detail-info {
    color: #ff8202;
}
/*table*/
.freeTrade-window {
    width: 100%;
}

.freeTrade-window th {
    height: 60px;
    text-align: center;
    background-color: #f2f8fc;
    border: 1px solid #d9d9d9;
}
.freeTrade-window td {
    height: 41px;
    text-align: center;
    border: 1px solid #d9d9d9;
}