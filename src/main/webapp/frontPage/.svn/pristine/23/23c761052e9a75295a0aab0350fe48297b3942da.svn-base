/*个人中心页面覆盖样式*/
.content .container {
    background-color: #fff;
    padding-bottom: 22px;
}

.footer {
    margin-top: 20px;
}

/* 新增申请人 */
.tab-hd {
	border-bottom: 1px solid #d9d9d9;
	padding-bottom: 15px;
	margin: 14px 0 38px 0;
}
.tab-target {
	font-size: 15px;
	padding: 0 16px 13px;
	border-bottom: 2px solid transparent;
	cursor: pointer;
}
.cur {
	font-weight: bold;
	border-bottom-color: #70c49a;
}
.tab-bd {
	min-height: 578px;
	margin-left: 18px;
}
.editor-item {
	margin-bottom: 20px;
}
.editor-label {
	display: inline-block;
	width: 70px;
}
.editor-input {
	width: 170px;
	height: 22px;
	line-height: 24px;
	color: #949494;
	border: 1px solid #c9c9c9;
	padding: 0 10px;
	margin-right: 30px;
}
.btn-save {
	width: 152px;
	height: 29px;
	line-height: 29px;
	color: #000;
	border: 0;
	background-color: #d7d7d7;
	margin-top: 26px;
	border-radius: 4px;
}