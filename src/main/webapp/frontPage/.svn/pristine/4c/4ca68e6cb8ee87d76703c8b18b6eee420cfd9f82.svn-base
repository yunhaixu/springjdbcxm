.con-hd {
	height: 25px;
	margin: 43px 0 29px 55px;
	font-size: 17px;
	font-weight: bold;
	color: #009241;
	padding-left: 35px;
	background: url(../images/gov-hall/goval-hdBg.png) 0 center no-repeat;
}

.method-item {
	width: 835px;
	margin-left: 99px;
}

.method1 {
	border-bottom: 1px solid #d9d9d9;
}

.method1>p {
	float: left;
	font-size: 15px;
	line-height: 50px;
}

.user-login-btn {
	float: left;
	width: 121px;
	height: 34px;
	padding-left: 34px;
	margin: 4px 0 0 36px;
	text-align: center;
	font-size: 13px;
	cursor: pointer;
	border: 1px solid #d0d0d0;
	background: url(../images/gov-hall/user.png) 22px 8px no-repeat;
	background-color: #ebebeb;
}

.user-login-btn:hover {
	color: #009241;
	background: url(../images/gov-hall/user-h.png) 22px 8px no-repeat;
	background-color: #daefe5;
}

.method2>p {
	font-size: 15px;
	line-height: 50px;
}

.input-item {
	width: 452px;
	position: relative;
	margin: 0 0 8px 110px;
	padding-bottom: 32px;
}

.input-item>label {
	float: left;
	text-align: right;
	height: 42px;
	width: 89px;
	line-height: 42px;
}

.input-item>input {
	float: left;
	width: 306px;
	height: 40px;
	padding: 0 10px;
	line-height: 40px;
	border: 1px solid #d1d1d1;
	margin-left: 22px;
}

.warn-txt {
	position: absolute;
	left: 112px;
	top: 42px;
	line-height: 32px;
	padding-left: 23px;
	font-size: 12px;
	color: #dc2828;
	background: url(../images/gov-hall/warn.png) 0 center no-repeat;
}

.grey-btnStyle {
	margin: 28px 0 0 299px;
}