
html #layui_layer_skinwebstylecss {
    display: none;
    position: absolute;
    width: 1989px;
}

body .layer-ext-subscribe .layui-layer-title {
    background: #587ed1;
    color: #fff;
    font-size: 16px;
}

body .layer-ext-subscribe .layui-layer-setwin .layui-layer-close1,
body .layer-ext-maintenance .layui-layer-setwin .layui-layer-close1,
body .layer-ext-publish .layui-layer-setwin .layui-layer-close1 {
    background: url("close.png") no-repeat center center;
}

body .layer-ext-maintenance .layui-layer-title {
    background: #45aedf;
    color: #fff;
    font-size: 16px;
}

body .layer-ext-publish .layui-layer-title {
    background: #45b8cf;
    color: #fff;
    font-size: 16px;
}

body .layer-ext-confirm .layui-layer-title,
body .layer-ext-alert .layui-layer-title {
    background: #fff;
    color: #0a407b;
    font-size: 16px;
    line-height: 35px;
    height: 35px;
    border-bottom-color: #ced2e1;
}

body .layer-ext-confirm .layui-layer-setwin,
body .layer-ext-alert .layui-layer-setwin {
    right: 8px;
    top: 10px;
}

body .layer-ext-confirm .layui-layer-content .layui-layer-ico {
    width: 38px;
    height: 38px;
    left: 80px;
    top: 40px;
}

body .layer-ext-alert .layui-layer-content .layui-layer-ico {
    left: 60px;
}

body .layer-ext-confirm .layui-layer-ico3 {
    background: url("confirm.png") no-repeat center center;
}

body .layer-ext-confirm .layui-layer-padding {
    padding: 40px 130px 40px 130px;
}

body .layer-ext-alert .layui-layer-padding {
    padding: 18px 100px 20px 100px;
}

body .layer-ext-confirm .layui-layer-btn,
body .layer-ext-alert .layui-layer-btn {
    text-align: center;
}

body .layer-ext-confirm .layui-layer-btn .layui-layer-btn0,
body .layer-ext-confirm .layui-layer-btn .layui-layer-btn1,
body .layer-ext-alert .layui-layer-btn .layui-layer-btn0 {
    width: 80px;
    height: 30px;
    line-height: 30px;
    border-radius: 3px;
    border: 0 none;
    color: #fff;
}

body .layer-ext-alert .layui-layer-btn .layui-layer-btn0 {
    width: 60px;
}

body .layer-ext-confirm .layui-layer-btn .layui-layer-btn0,
body .layer-ext-alert .layui-layer-btn .layui-layer-btn0 {
    background: #fc6c53;
}

body .layer-ext-confirm .layui-layer-btn .layui-layer-btn1 {
    background: #45b8cf;
}

body .layer-ext-confirm .layui-layer-content .layui-layer-input {
    width: 420px;
    height: 80px;
    max-width: 420px;
    max-height: 80px;
    min-width: 420px;
    min-height: 80px;
    border: 1px solid #ccc;
    outline: 0 none;
    padding: 10px;
}
